<% @jobs.each do |job| %>

<h2><%= job.title %></h2>

<p><%= job.job_desc %></p>

<p><%= job.seniority %></p>

<%= link_to "編輯職缺", edit_company_job_path(@company,job), :class => "btn btn-primary" %>

<%= link_to "刪除職缺", company_job_path(@company,job), :method => :delete, :class => "btn btn-danger" %>

<% end %>
