
<div class="container">
  <h1>
    <%= current_user.email %>'s Porfile
  </h1>
  <div class="col-md-2">
    <%= render :partial => './resumes/side_bar' %>
  </div>
  <div class="col-md-10">
    <%= form_for current_user, url: user_path(current_user), method: :patch do |f| %>
    <div class="field">
      <%= f.label :名字 %><br />
      <%= f.text_field :name, autocomplete: "off" %>
    </div>

    <div class="field">
      <%= f.label :電話 %><br />
      <%= f.text_field :tel, autocomplete: "off" %>
    </div>

    <br>
    <div class="actions">
      <%= f.submit "確認", class: "btn btn-primary" %>
    <% end %>
    </div> 
  </div>
</div>